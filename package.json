{
  "name": "vitrine",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "start": "npm-run-all --parallel start:backend start:frontend",
    "start:backend": "npm --prefix backend run dev",
    "start:frontend": "npm --prefix frontend run dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "type": "commonjs"
}
